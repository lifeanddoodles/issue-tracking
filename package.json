{
  "name": "issue-tracking",
  "type": "module",
  "version": "1.0.0",
  "description": "Fullstack app for issue tracking using the MERN stack.",
  "main": "server.ts",
  "scripts": {
    "start": "ts-node backend/server.ts",
    "server": "nodemon backend/server.ts",
    "client": "npm start --prefix frontend",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "test": "npm run test --prefix frontend"
  },
  "keywords": [
    "node",
    "react",
    "mongodb",
    "express",
    "typescript"
  ],
  "author": "Sandra Vargas",
  "license": "ISC",
  "dependencies": {
    "@types/express": "^4.17.17",
    "@types/node": "^20.5.6",
    "bcrypt": "^5.1.1",
    "connect-mongo": "^5.0.0",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-async-handler": "^1.2.0",
    "express-session": "^1.17.3",
    "jsonwebtoken": "^9.0.2",
    "mongoose": "^7.4.5",
    "passport": "^0.6.0",
    "passport-google-oauth20": "^2.0.0",
    "passport-jwt": "^4.0.1",
    "passport-local": "^1.0.0"
  },
  "devDependencies": {
    "@testing-library/react-hooks": "^8.0.1",
    "@types/bcrypt": "^5.0.0",

{
  "name": "issue-tracking",
  "type": "module",
  "version": "1.0.0",
  "description": "Fullstack app for issue tracking using the MERN stack.",
  "main": "server.ts",
  "scripts": {
    "start": "ts-node backend/server.ts",
    "server": "nodemon backend/server.ts",
    "client": "npm start --prefix frontend",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "test": "npm run test --prefix frontend"
  },
  "keywords": [
    "node",
    "react",
    "mongodb",
    "express",
    "typescript"
  ],
  "author": "Sandra Vargas",
  "license": "ISC",
  "dependencies": {
    "@types/express": "^4.17.17",
    "@types/node": "^20.5.6",
    "bcrypt": "^5.1.1",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-async-handler": "^1.2.0",
    "mongoose": "^7.4.5"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.0",
    "@types/cors": "^2.8.13",
    "concurrently": "^8.2.1",
    "nodemon": "^3.0.1",
    "ts-node": "^10.9.1",
    "typescript": "^5.2.2"
  }
}

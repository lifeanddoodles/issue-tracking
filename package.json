    "@types/express-session": "^1.17.7",
    "@types/jsonwebtoken": "^9.0.3",
    "@types/passport": "^1.0.12",
    "@types/passport-google-oauth20": "^2.0.11",
    "@types/passport-jwt": "^3.0.9",
    "@types/passport-local": "^1.0.35",
    "concurrently": "^8.2.1",
    "nodemon": "^3.0.1",
    "ts-node": "^10.9.1",
    "typescript": "^5.2.2",
    "vitest-preview": "^0.0.1"
  }
}

{
  "name": "issue-tracking-backend",
  "version": "1.0.0",
  "description": "Backend for an issue tracking app using the MERN stack.",
  "main": "server.ts",
  "type": "module",
  "exports": "./dist/server.js",
  "types": "./dist/server.d.ts",
  "scripts": {
    "start": "node --no-warnings=ExperimentalWarning --loader ts-node/esm -r dotenv/config ./server.ts",
    "server": "nodemon --watch ./ -e ts --exec \"npm run start\""
